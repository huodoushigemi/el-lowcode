    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue-macros/volar": "^0.12.3",
    "rollup": "^3.27.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "tslib": "^2.6.1",
    "typescript": "^5.0.2",
    "unplugin-vue": "^4.2.5",
    "unplugin-vue-macros": "^2.4.0",
    "vite": "^4.4.5",
    "vitepress": "1.0.0-beta.7",
    "vue-tsc": "^1.8.5"
